@model WebUI.Models.MessageViewModel
@{
    ViewBag.Title = "WalletDataPartialView";
}


    <div class="lds-roller" id="loading" role="status" style="display: none"><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div></div>
    <div id="KeyValues">
        <div class=" row form-group">
            <div class="col-md-2">
                <button type="button" class="btn btn-info" data-toggle="collapse" data-target="#privateKey">Show Private Key</button>
                <div id="privateKey" class="collapse">
                    <p> [@Model.n , @Model.d] </p>
                </div>
            </div>
            <div class="col-md-2">
                <button type="button" class="btn btn-info" data-toggle="collapse" data-target="#publicKey">Show Public Key</button>
                <div id="publicKey" class="collapse">
                    <p>[@Model.n , @Model.e]</p>
                </div>
            </div>
        </div>
    </div>
