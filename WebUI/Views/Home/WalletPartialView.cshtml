
@{
    ViewBag.Title = "WalletPartialView";

}

<script>
    
    function hide(elementID) {
        document.getElementById(elementID).style.display = 'none';
    }
    function show(elementID) {
        var element = document.getElementById(elementID);
        element.style.display = 'inline-block';
    }

</script>

<div class="jumbotron">
    @Html.Action("GetWalletData")
    <div class="container">
        <div class="row form-group">
            <div class="col-md-12">
                @using (Ajax.BeginForm(new AjaxOptions { UpdateTargetId = "KeyValues", LoadingElementId = "loading", LoadingElementDuration = 300, Url = Url.Action("GetWalletData"), OnBegin = "hide('KeyValues')", OnComplete = "show('KeyValues')", HttpMethod = "Post" }))
                {
                    @Html.AntiForgeryToken()
                    <button type="submit" class="btn btn-primary">Generate !</button>
                }
            </div>
        </div>
    </div>
</div>